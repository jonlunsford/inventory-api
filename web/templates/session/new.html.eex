<%= form_for @conn, session_path(@conn, :create), [as: :user], fn f -> %>
  <label>
    Email: <%= text_input f, :email %>
    <%= error_tag f, :email %>
  </label>

  <label>
    Password: <%= password_input f, :password %>
    <%= error_tag f, :password %>
  </label>

  <%= submit "Submit" %>
<% end %>
